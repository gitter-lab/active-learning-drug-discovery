{
  "pre_subsampling_stage" : {
    "stage_results_dir": "/presubsampling_stage/"
  },
  
  "subsampling_stage" : {
    "stage_results_dir": "/subsampling_stage/",
    "task_run_dir": "/task_{}/run_{}/",
    
    "run_range": [0, 500],
    "runs_per_process": 25,
    "min_sample_per_cluster": 2,
    "cluster_sample_factor": 0.1
  },
  
  "analysis_stage" : {
    "stage_results_dir": "/analysis_stage/",
    "comparison_params": {
      "metric": "NEF 1.0%",
      "good_run_cutoff": 0.5,
      "important_indices_cutoff": 0.5,
      "task_important_indices_strategy": "union"
    }
  },
  
  "model": {
    "type": "supervised",
    "class": "sklean_randomforest",
    "params" : {
        "n_estimators": 50,
        "max_features": "log2",
        "min_samples_leaf": 1,
        "n_jobs": 1,
        "class_weight": "balanced",
        "random_state": 20183112,
        "oob_score": false,
        "verbose": 1
    }
  },
  
  "training_data_params": {
    "data_loader": "CSVLoader",
    "data_path_format": "../datasets/training_data/iter_{}.csv"
  },
    
  "unlabeled_data_params": {
    "data_loader": "CSVLoader",
    "data_path_format": "../datasets/unlabeled_data/unlabeled_{}.csv"
  },
  
  "next_batch_selector_params": {
    "batch_size": 100,
    "activity_threshold": 0.75,
    "exploitation_threshold": 0.5,
    "exploitation_alpha": 0.5,
    "exploration_threshold": 0.5,
    "exploration_beta": 0.5
  },
    
  "common": {
    "params_set_results_dir": "../params_results/params_set_0_results/",
    "iter_results_dir": "/iter_{}/",
    "eval_dest_file": "/eval.csv",
    
    "task_names": "PriA-SSB Activity",
    "smile_col_name": "rdkit SMILES",
    "feature_name": "Morgan FP_2_1024",
    "cluster_col_name": "Murcko Scaffold ID",
    "molecule_id_col_name": "Molecue ID",
    "cost_col_name": "Cost",
    
    "metrics_params": {
      "w_novelty": 0.5,
      "perc_vec": [0.001, 0.0015, 0.005, 0.01, 0.02, 0.05, 0.1, 0.2],
      "n_tests_list": [100, 250, 500, 1000, 2500]
    }
  }
}